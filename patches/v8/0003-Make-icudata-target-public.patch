---
 bazel/BUILD.icu | 1 +
 1 file changed, 1 insertion(+)

diff --git a/bazel/BUILD.icu b/bazel/BUILD.icu
index 2ae79a5784..4843ea09f9 100644
--- a/bazel/BUILD.icu
+++ b/bazel/BUILD.icu
@@ -5,6 +5,7 @@
 filegroup(
     name = "icudata",
     srcs = ["common/icudtl.dat"],
+    visibility = ["//visibility:public"],
 )
 
